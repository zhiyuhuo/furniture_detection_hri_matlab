function res = furniture_rbf_classify(sample, model)

sv = model.SupportVectors;
a = model.Alpha;
b = model.Bias;

scalefactor = model.ScaleData.scaleFactor;
shift = model.ScaleData.shift;

x = model.*scalefactor + shift;
lb = model.GroupNames(model.SupportVectorIndices);

r = 0;

for i = 1:size(sv, 1)
    r = r + a(i) * lb(i) * lbkernel_rbf(x, sv(i,:))
end

end